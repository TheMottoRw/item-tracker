<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itemtracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ItemTracker"
        android:usesCleartextTraffic="true">
        <activity android:name=".SubmitDocument" />
        <activity android:name=".AdminForm" />
        <activity
            android:name=".SuperadminNavigation"
            android:label="Item tracker" />
        <activity android:name=".DocumentTypeForm" />
        <activity android:name=".DeclarationView" />
        <activity android:name=".DeclarationForm" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".SubmittedDocumentView" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>